{
    "name": "miranj/craft-contact-form-tuner",
    "description": "Adds a bunch of additional controls (cc, bcc, reply-to, templates, plain text control) to the Craft CMS Contact Form plugin.",
    "version": "2.0.0",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "craftcms",
        "contact",
        "form",
        "email"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Miranj",
            "homepage": "https://miranj.in/"
        }
    ],
    "support": {
        "docs": "https://github.com/miranj/craft-contact-form-tuner/blob/master/README.md",
        "issues": "https://github.com/miranj/craft-contact-form-tuner/issues"
    },
    "require": {
        "craftcms/cms": "^4.0",
        "craftcms/contact-form": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "miranj\\contactformtuner\\": "src/"
        }
    },
    "extra": {
        "name": "Contact Form Tuner",
        "handle": "contact-form-tuner",
        "hasCpSettings": true,
        "changelogUrl": "https://raw.githubusercontent.com/miranj/craft-contact-form-tuner/master/CHANGELOG.md",
        "class": "miranj\\contactformtuner\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
